{
    "name": "mitosis/cart",
    "description": "Mitosis Cart Module",
    "type": "library",
    "license": "MIT",
   
    "minimum-stability": "dev",
    "prefer-stable": true,
    
    "require": {
        "php": "^7.2",
        "konekt/concord": "~1.4"        
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "phpunit/phpunit": "6.2 - 7.5|~8.0|~9.0",
        "orchestra/testbench": "~3.5|~4.0|~5.0",
        "konekt/enum": "~2.1"
    },
    "autoload": {
        "psr-4": { "Mitosis\\Cart\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Mitosis\\Cart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Cart": "Mitosis\\Cart\\Facades\\Cart"
            }
        }
    }
}
