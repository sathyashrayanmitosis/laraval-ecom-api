{
    "name": "mitosis/category",
    "description": "Mitosis Category Module",
    "type": "library",
    "license": "MIT",
    
    "require": {
        "php": "^7.2",
        "konekt/concord": "~1.4",
        "cviebrock/eloquent-sluggable": "4.3 - 7"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2 - 7.5|~8.0|~9.0",
        "orchestra/testbench": "~3.5|~4.0|~5.0"
    },
    "autoload": {
        "psr-4": { "Mitosis\\Category\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Mitosis\\Category\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
