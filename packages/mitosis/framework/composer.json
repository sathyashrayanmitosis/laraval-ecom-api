{
    "name": "mitosis/framework",
    "description": "E-commerce Framework for Laravel",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.5|~6.0|~7.0",
        "konekt/appshell": "~1.5",
        "spatie/laravel-medialibrary": "^7.3"
        
    },
    "autoload": {
        "psr-4": {
            "Mitosis\\Framework\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "~3.5|~4.0|~5.0",
        "phpunit/phpunit" : "6.2 - 7.5|~8.0|~9.0",
        "mockery/mockery": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Mitosis\\Framework\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
